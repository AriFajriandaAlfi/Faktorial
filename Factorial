#include <iostream>
#include <stdio.h>

using namespace std;

void kali(int n){
    int x = 1;
    int sum = n;
    for (int i = 1; i <= sum ; i++ ){
        x=x*i;

    }
    cout << "Faktorial : "<<endl;
    cout << x;
}


int main()
{
    int fact;
    cout << "Input : " << endl;
    cin >> fact;
    kali(fact);


    return 0;
}
